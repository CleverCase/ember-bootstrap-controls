{{#section.subsection name='<%= camelizedModuleName %>'}}
  {{#freestyle-collection
    defaultKey='Basic Usage'
    inline=false
    as |collection|
  }}
    {{#collection.variant key='Basic Usage'}}
      {{#freestyle-usage 'basic-<%= dasherizedModuleName %>' name='Basic <%= classifiedPackageName %>'}}
        {{<%= dasherizedModuleName %>}}
      {{/freestyle-usage}}
    {{/collection.variant}}
    {{#collection.variant key='Advanced Usage'}}
      {{#freestyle-usage 'advanced-<%= dasherizedModuleName %>' name='Advanced <%= classifiedPackageName %>'}}
        {{<%= dasherizedModuleName %>}}
        {{freestyle-prop-types data=data propDefinitions=propDefinitions}}
      {{/freestyle-usage}}
    {{/collection.variant}}
  {{/freestyle-collection}}
{{/section.subsection}}
