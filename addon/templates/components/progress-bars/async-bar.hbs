{{#let (component 'progress-bars/-bar'
  classNames='progress-bar-striped progress-bar-animated'
  minimumValue=0
  maximumValue=100
  currentValue=100
  label=label
) as |progressBar|}}
  {{#if hasBlock}}
    {{yield (hash
      bar=progressBar
    )}}
  {{else}}
    {{progressBar}}
  {{/if}}
{{/let}}
