  (hash
    checkbox=(component 'bootstrap-inputs/-checkbox' formDisabled=formDisabled)
    date=(component 'bootstrap-inputs/-date' formDisabled=formDisabled)
    email=(component 'bootstrap-inputs/-email' formDisabled=formDisabled)
    floatingLabelInput=(component 'bootstrap-inputs/-floating-label-input' formDisabled=formDisabled)
    number=(component 'bootstrap-inputs/-number' formDisabled=formDisabled)
    password=(component 'bootstrap-inputs/-password' formDisabled=formDisabled)
    radioGroup=(component 'bootstrap/radio-group' formDisabled=formDisabled)
    range=(component 'bootstrap-inputs/-range' formDisabled=formDisabled)
    search=(component 'bootstrap-inputs/-search' formDisabled=formDisabled)
    select=(component 'bootstrap-inputs/-select' formDisabled=formDisabled)
    tel=(component 'bootstrap-inputs/-tel' formDisabled=formDisabled)
    text=(component 'bootstrap-inputs/-text' formDisabled=formDisabled)
    time=(component 'bootstrap-inputs/-time' formDisabled=formDisabled)
