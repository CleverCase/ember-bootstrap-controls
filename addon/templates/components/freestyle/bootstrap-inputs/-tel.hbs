{{#section.subsection name='Telephone Input'}}
  {{#freestyle-collection
    defaultKey='Basic Usage'
    inline=false
    as |collection|
  }}
    {{#collection.variant key='Basic Usage'}}
      {{#freestyle-usage 'basic-tel-input' name='Basic Telephone Input'}}
        {{bootstrap-inputs/-tel
          label="Phone #"
          value=basicValue}}
      {{/freestyle-usage}}
    {{/collection.variant}}
    {{#collection.variant key='Advanced Usage'}}
      {{#freestyle-usage 'advanced-tel-input' name='Advanced Telephone Input'}}
        {{bootstrap-inputs/-tel
          autocomplete=data.autocomplete
          help=data.help
          disabled=data.disabled
          label=data.label
          maxlength=data.maxlength
          minlength=data.minlength
          placeholder=data.placeholder
          readonly=data.readonly
          required=data.required
          srOnly=data.srOnly
          tabIndex=data.tabIndex
          value=data.value}}
        {{freestyle-prop-types data=data propDefinitions=propDefinitions}}
      {{/freestyle-usage}}
    {{/collection.variant}}
  {{/freestyle-collection}}
{{/section.subsection}}
