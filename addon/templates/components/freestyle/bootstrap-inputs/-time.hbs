{{#section.subsection name='Time Input'}}
  {{#freestyle-collection
    defaultKey='Basic Usage'
    inline=false
    as |collection|
  }}
    {{#collection.variant key='Basic Usage'}}
      {{#freestyle-usage 'basic-bootstrap-inputs-time' name='Basic Time Input'}}
        {{bootstrap-inputs/-time
          label="Time"}}
      {{/freestyle-usage}}
    {{/collection.variant}}
    {{#collection.variant key='Advanced Usage'}}
      {{#freestyle-usage 'advanced-bootstrap-inputs-time' name='Advanced Time Input'}}
        {{bootstrap-inputs/-time
          label="Time"}}
        {{freestyle-prop-types data=data propDefinitions=propDefinitions}}
      {{/freestyle-usage}}
    {{/collection.variant}}
  {{/freestyle-collection}}
{{/section.subsection}}
