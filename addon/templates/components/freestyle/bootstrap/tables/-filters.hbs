      {{#freestyle-usage "basic-bootstrap-tables--filters" name="Basic @wildlandLabs/emberBootstrapControls"}}
        {{bootstrap/tables/-filters}}
      {{/freestyle-usage}}
    {{/collection.variant}}
    {{#collection.variant key="Advanced Usage"}}
      {{#freestyle-usage "advanced-bootstrap-tables--filters" name="Advanced @wildlandLabs/emberBootstrapControls"}}
        {{bootstrap/tables/-filters}}
        {{freestyle-prop-types data=data propDefinitions=propDefinitions}}
      {{/freestyle-usage}}
    {{/collection.variant}}
  {{/freestyle-collection}}
{{/section.subsection}}
